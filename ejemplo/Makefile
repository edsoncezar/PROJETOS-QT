dist: 
	@$(CHK_DIR_EXISTS) compilar/ejemplo1.0.0 || $(MKDIR) compilar/ejemplo1.0.0 
	$(COPY_FILE) --parents $(SOURCES) $(DIST) compilar/ejemplo1.0.0/ && $(COPY_FILE) --parents src/login.h compilar/ejemplo1.0.0/ && $(COPY_FILE) --parents src/main.cpp src/login.cpp compilar/ejemplo1.0.0/ && (cd `dirname compilar/ejemplo1.0.0` && $(TAR) ejemplo1.0.0.tar ejemplo1.0.0 && $(COMPRESS) ejemplo1.0.0.tar) && $(MOVE) `dirname compilar/ejemplo1.0.0`/ejemplo1.0.0.tar.gz . && $(DEL_FILE) -r compilar/ejemplo1.0.0


