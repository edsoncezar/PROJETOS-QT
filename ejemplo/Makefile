	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/dhabyx//bin/ || $(MKDIR) $(INSTALL_ROOT)/home/dhabyx//bin/ 
	-$(INSTALL_FILE) /home/dhabyx/Desarrollo/qdevelop/ejemplo/bd.db $(INSTALL_ROOT)/home/dhabyx//bin/


uninstall_database:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/home/dhabyx//bin/bd.db
	-$(DEL_DIR) $(INSTALL_ROOT)/home/dhabyx//bin/ 


install_target: first FORCE
	@$(CHK_DIR_EXISTS) $(INSTALL_ROOT)/home/dhabyx//bin/ || $(MKDIR) $(INSTALL_ROOT)/home/dhabyx//bin/ 
	-$(INSTALL_PROGRAM) "bin/$(QMAKE_TARGET)" "$(INSTALL_ROOT)/home/dhabyx/bin/$(QMAKE_TARGET)"
	-strip "$(INSTALL_ROOT)/home/dhabyx/bin/$(QMAKE_TARGET)"

uninstall_target:  FORCE
	-$(DEL_FILE) "$(INSTALL_ROOT)/home/dhabyx/bin/$(QMAKE_TARGET)"
	-$(DEL_DIR) $(INSTALL_ROOT)/home/dhabyx//bin/ 


install:  install_database install_target  FORCE

uninstall: uninstall_database uninstall_target   FORCE

