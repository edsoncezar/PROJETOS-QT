####### Sub-libraries

distclean: clean
	-$(DEL_FILE) $(TARGET) 
	-$(DEL_FILE) Makefile


/opt/qt4/bin/moc:
	(cd $(QTDIR)/src/tools/moc && $(MAKE))

mocclean: compiler_moc_header_clean compiler_moc_source_clean

mocables: compiler_moc_header_make_all compiler_moc_source_make_all

compiler_moc_header_make_all: compilar/moc_login.cpp
compiler_moc_header_clean:
	-$(DEL_FILE) compilar/moc_login.cpp
compilar/moc_login.cpp: src/login.h \
		/opt/qt4/bin/moc
	/opt/qt4/bin/moc $(DEFINES) $(INCPATH) src/login.h -o compilar/moc_login.cpp

compiler_moc_source_make_all:
compiler_moc_source_clean:
compiler_clean: compiler_moc_header_clean 

####### Compile

compilar/main.o: src/main.cpp src/login.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o compilar/main.o src/main.cpp

compilar/login.o: src/login.cpp src/login.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o compilar/login.o src/login.cpp

compilar/moc_login.o: compilar/moc_login.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o compilar/moc_login.o compilar/moc_login.cpp

